{
  "manifest_version": 3,
  "name": "AdventJs Calendar",
  "version": "1.0",
  "description": "calendario para revisar los retos del adventjs de midudev",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}